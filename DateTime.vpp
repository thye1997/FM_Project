class DateTime

	types
		public DateTime :: date: Date
						   time: Time;

	instance variables
		public dateTime: DateTime;
		
	operations
		public DateTime: () ==> DateTime
			DateTime () == (
				dateTime := mk_DateTime(new Date(), new Time());
			);

		public DateTime: bool ==> DateTime
			DateTime (init) == (
				dateTime := mk_DateTime(new Date(), new Time());
			);
			
end DateTime